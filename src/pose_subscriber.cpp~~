#include "ros/ros.h"                          // ROS Default Header File
#include "ros_tutorials_topic/MsgTutorial.h"  // MsgTutorial Message File Header. The header file is automatically created when building the package.
#include <ros_tutorials_topic/robotYawMsg.h>
void msgCallback(const ros_tutorials_topic::robotYawMsg msg)
{
	ROS_INFO("test");
	ROS_INFO("%d",msg.data);
}

int main(int argc, char **argv)                         // Node Main Function
{
	ros::init(argc, argv, "topic_subscriber");            // Initializes Node Name
	ros::NodeHandle nh;                                   // Node handle declaration for communication with ROS system
	ros::Subscriber ros_tutorial_sub = nh.subscribe("robot_pose_yaw", 100, msgCallback);

	ros::spin();

	return 0;
}
