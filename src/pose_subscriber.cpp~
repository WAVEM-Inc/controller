#include "ros/ros.h"                          // ROS Default Header File
#include <ros_tutorials_topic/robotYawMsg.h>
void msgCallback(const mpc_navigation::robotYawMsg msg)
{
	ROS_INFO("test");
	ROS_INFO("%d",msg.data);
}

int main(int argc, char **argv)                         // Node Main Function
{
	ros::init(argc, argv, "mpc_yaw");            // Initializes Node Name
	ros::NodeHandle nh;                                   // Node handle declaration for communication with ROS system
	ros::Subscriber ros_tutorial_sub = nh.subscribe("robot_pose_yaw_1", 100, msgCallback);

	ros::spin();

	return 0;
}
